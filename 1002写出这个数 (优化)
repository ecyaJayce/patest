#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<string.h>
int main()
{
	int t = 0;
	int i = 0;
	int sum = 0;
	int num[4] = {};
	char arr[101];
	char pingyin[][5] = { "ling","yi","er","san","si","wu","liu","qi","ba","jiu" };
	scanf("%s", &arr);
	for (i = 0; i < strlen(arr); i++)
	{
		sum = sum + arr[i] - '0';
	}
	i = 0;
	while (sum)
	{
		t = sum % 10;
		sum = sum / 10;
		num[i++] = t;
	}
	for (i=i-1; i > 0; i--)
	{
		printf("%s ",pingyin[num[i]]);
	}
	printf("%s", pingyin[num[0]]);
	return 0;
}
