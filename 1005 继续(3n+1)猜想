#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<string.h>
#include<stdlib.h>
int cmp_int(const void* e1, const void* e2)
{
	return *((int*)e2) - *((int*)e1);
}
int belong(int input, int* arr1,int sz)
{
	int i = 0;
	for (i = 0; i < sz; i++)
	{
		if (input == arr1[i])
			return i;
	}
	return sz + 1;
}
void feb(int input,int* arr1,int sz)
{
	int i = 0;
	if (input == 0)
		;
	else
	{
		while (input != 1)
		{
			if (input % 2 == 0)
			{
				input = input / 2;
				arr1[belong(input, arr1, sz)] = 0;
			}
			else
			{
				input = (input * 3 + 1) / 2;
				arr1[belong(input, arr1, sz)] = 0;
			}
		}
	}
}
int pow(int x, int y)
{
		int sum = 1;
		int i = 0;
		for (i = 0; i < y; i++)
		{
			sum = sum * x;
		}
		return sum;
}
int main()
{
	int input = 0;
	int i = 0;
	int j = 0;
	int t = 0;
	int wei = 0;
	int arr1[101] = {};
	char arr2[400] = {};
	int point[100] = {};
	scanf("%d", &input);
	getchar();
	scanf("%[0-9 ]", arr2);
	int len = strlen(arr2);
	arr2[len] = ' ';
	for (i = 0; i < len+2; i++)//12 2 wei=2 i=2
	{
		if (arr2[i] != ' '&&arr2[i]!='\n')
		{
			wei++;
			continue;
		}
		else
		{
			int sum = 0;
			for (j = 0; j < wei; j++)
			{
				sum = sum + (arr2[i - j-1]-'0') * pow(10, j);//99
			}
			arr1[t++] = sum;
			wei = 0;
		}
	}//获取输入得若干个数字到 arr1数组
	for (i = 0; i < input; i++)
	{
		feb(arr1[i],arr1,input);
	}
	qsort(arr1, input, sizeof(arr1[0]), cmp_int);

	int number = 0;
	for (i = 0; i < input; i++)
	{
		if (arr1[i] != 0)
			number++;
		else
			break;
	}
	for (i = 0; i < number - 1; i++)
	{
		if (arr1[i] != 0)
			printf("%d ", arr1[i]);
	}
	printf("%d", arr1[number - 1]);
	return 0;
}
