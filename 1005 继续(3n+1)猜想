#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<stdlib.h>
int cmp_int(const void* e1, const void* e2)
{
	return *((int*)e2) - *((int*)e1);
}
int belong(int input, int* arr1, int sz)
{
	int i = 0;
	for (i = 0; i < sz; i++)
	{
		if (input == arr1[i])
			return i;
	}
	return sz + 1;
}
void feb(int input, int* arr1, int sz)
{
	int i = 0;
	if (input == 0)
		;
	else
	{
		while (input != 1)
		{
			if (input % 2 == 0)
			{
				input = input / 2;
				arr1[belong(input, arr1, sz)] = 0;
			}
			else
			{
				input = (input * 3 + 1) / 2;
				arr1[belong(input, arr1, sz)] = 0;
			}
		}
	}
}
int pow(int x, int y)
{
	int sum = 1;
	int i = 0;
	for (i = 0; i < y; i++)
	{
		sum = sum * x;
	}
	return sum;
}
int main()
{
	int input = 0;
	int i = 0;
	int number = 0;
	int arr1[101] = {};
	scanf("%d", &input);
	getchar();
	for (i = 0; i < input; i++)
	{
		scanf("%d", &arr1[i]);
	}
	for (i = 0; i < input; i++)
	{
		feb(arr1[i], arr1, input);
	}
	qsort(arr1, input, sizeof(arr1[0]), cmp_int);
	for (i = 0; i < input; i++)
	{
		if (arr1[i] != 0)
			number++;
		else
			break;
	}
	for (i = 0; i < number - 1; i++)
	{
		if (arr1[i] != 0)
			printf("%d ", arr1[i]);
	}
	printf("%d", arr1[number - 1]);
	return 0;
}
