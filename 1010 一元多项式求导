#include<stdio.h>
int main()
{
	int xishu, zhishu;
	scanf("%d%d",&xishu,&zhishu);
	if (zhishu == 0)
		printf("0 0");
	else
		printf("%d %d", xishu * zhishu, zhishu - 1);
	while (scanf("%d %d", &xishu, &zhishu) != EOF && zhishu != 0)
		{
					printf(" %d %d", xishu * zhishu, zhishu - 1);
		}
	return 0;
}
