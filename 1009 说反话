#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
int main()
{
	int i = 0;
	int j = 0;
	int t = 0;
	char text[81] = {};
	int num[41] = {0};
	scanf("%[A-Z a-z]", text);
	for (i = 0; i < 81; i++)
	{
		if (text[i] == ' ')
			num[++j] = i;
		else if (text[i] == '\0')
		{
			num[++j] = i;
			break;
		}
	}
	for (i = j; i > 1; i--)
	{
			for (t = num[i-1]+1; t < num[i]; t++)
			{
				printf("%c", text[t]);
			}
			printf(" ");
	}
	for (t = num[0]; t < num[1]; t++)
	{
		printf("%c", text[t]);
	}
	return 0;
}
