#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
int M1_J = 0;
int M1_B = 0;
int M1_C = 0;
int M2_J = 0;
int M2_B = 0;
int M2_C = 0;
int cmp(char M1, char M2)
{
	if (M1 == 'C')
	{
		if (M2 == 'C')
			return 0;
		else if (M2 == 'J')
		{
			M1_C++;
			return 1;
		}
		else if (M2 == 'B')
		{
			M2_B++;
			return 2;
		}
	}
	if (M1 == 'J')
	{
		if (M2 == 'C')
		{
			M2_C++;
			return 2;
		}
		else if (M2 == 'J')
			return 0;
		else if (M2 == 'B')
		{
			M1_J++;
			return 1;
		}
	}
	if (M1 == 'B')
	{
		if (M2 == 'C')
		{
			M1_B++;
			return 1;
		}
		else if (M2 == 'J')
		{
			M2_J++;
			return 2;
		}
		else if (M2 == 'B')
			return 0;
	}
}

int main()
{
	int i = 0;
	int count = 0;
	int tie=0;
	int win = 0;
	int lose = 0;
	scanf("%d", &count);
	getchar();
	char M1;
	char M2;
	for (i = 0; i < count; i++)
	{
		scanf("%c %c", &M1, &M2);
		getchar();
		switch(cmp(M1, M2))
		{
		case 0:
			tie++;
			break;
		case 1:
			win++;
			break;
		case 2:
			lose++;
			break;
		}
	}
	printf("%d %d %d\n", win, tie, lose);
	printf("%d %d %d\n", lose, tie, win);
	if (M1_B >= M1_C)
	{
		if(M1_B>=M1_J)
			printf("B");
		else
			printf("J");
	}
	else
	{
		if (M1_C >= M1_J)
			printf("C");
		else
			printf("J");
	}
	printf(" ");
	if (M2_B >= M2_C)
	{
		if (M2_B >= M2_J)
			printf("B");
		else
			printf("J");
	}
	else
	{
		if (M2_C >= M2_J)
			printf("C");
		else
			printf("J");
	}
	return 0;
}
