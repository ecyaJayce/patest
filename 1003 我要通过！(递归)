#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
#include<string.h>
int req1(char* pat,int len)
{
	int j = 0;
	for (j = 0; j < len; j++)
	{
		if (pat[j] != 'P' && pat[j] != 'A' && pat[j] != 'T')
			return 0;
	}
	return 1;
}
int req2(char* pat, int len)
{
	if (len % 2 == 1)
	{
		int j = 0;
		for (j = 0; j < ((len - 3) / 2); j++)
		{
			if (pat[j] != 'A')
				return 0;
		}
		if (pat[((len - 3) / 2)] != 'P')
			return 0;
		if (pat[((len - 3) / 2)+1] != 'A')
			return 0;
		if (pat[((len - 3) / 2+2)] != 'T')
			return 0;
		for (j = ((len - 3) / 2 + 3); j < len; j++)
		{
			if (pat[j] != 'A')
				return 0;
		}
			return 1;
	}
	else
		return 0;
}
int req3(char* arr, int len)
{
	int a = 0;
	int b = 0;
	int c = 0;
	int k = 0;
	int i = 0;
	while (arr[k]=='A')
	{
		a++;
		k++;
	}
	if (arr[k] == 'T')
		return 0;
	k++;
	while (arr[k] == 'A')
	{
		b++;
		k++;
	}
	if (arr[k] == 'P')
		return 0;
	b--;
	k++;
	while (arr[k] == 'A')
	{
		c++;
		k++;
	}
	if (arr[k] == 'P'|| arr[k] == 'T')
		return 0;
	c = c - a;
	char New[101] = {};
	for (i = 0; i < a; i++)
	{
		New[i] = 'A';
	}
	New[a] = 'P';
	for (i = a + 1; i < a+b+1; i++)
	{					  	
		New[i] = 'A';
	}
	New[a + b + 1] = 'T';
	for (i = a +b+2; i < a+b+c+2; i++)
	{					  	
		New[i] = 'A';
	}
	if (b > 1)
		return req3(New, a + b + c + 2);
	else
		return req2(New, a + b + c  + 2);
}
int main()
{
	int i ,len;
	int count = 0;
	char pat[101];
	scanf("%d", &count);
	for (i = 0; i < count; i++)
	{
		scanf("%s", pat);
		getchar();
		len=strlen(pat);
		if (len>=3)
		{
			if (req1(pat, len))
			{
				if (req2(pat, len))
				{
					printf("YES\n");
				}
				else
				{
					if (req3(pat, len))
					{
						printf("YES\n");
					}
					else
					{
						printf("NO\n");
					}
				}
			}
			else
				printf("NO\n");
		}
		else
		{
			printf("NO\n");
		}
	}
	return 0;
}
